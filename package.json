{
  "name": "white-estate-web",
  "version": "1.0.0",
  "description": "",
  "repository": {
    "type": "git",
    "url": "https://github.com"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve --live-reload=false --configuration=local --port=3000",
    "start:docker-dev": "ng serve --live-reload=false --configuration=docker-dev --port=3000",
    "create-environment": "node --require ts-node/register create-environment.ts",
    "build": "ng build --configuration=local",
    "build:prod": "ng build --prod",
    "build:docker": "ng build --prod --configuration=docker",
    "build:server": "ng run we-web:server:local && npm run webpack:server",
    "build:server:watch": "nodemon --exec \"npm run build:server && npm run serve:ssr\"",
    "build:server:prod": "ng run we-web:server:production && npm run webpack:server",
    "lint": "ng lint --fix",
    "build:prerender": "ng build --prod && npm run build:server:prod && npm run generate:prerender",
    "build:ssr": "ng build --configuration=local && npm run build:server",
    "build:ssr:prod": "ng build --prod && npm run build:server:prod",
    "build:ssr:docker": "ng build --prod --configuration=docker && ng run we-web:server:docker && npm run webpack:server",
    "generate:prerender": "cd dist && node prerender",
    "webpack:server": "webpack --config webpack.server.config.js --progress --colors",
    "serve:prerender": "cd dist/browser && http-server",
    "serve:ssr": "node dist/server",
    "clean:dist": "rimraf dist",
    "debug:build": "node-nightly --inspect --debug-brk node_modules/webpack/bin/webpack.js",
    "debug:build:prod": "node-nightly --inspect --debug-brk node_modules/webpack/bin/webpack.js  --config webpack.prod.config.ts",
    "debug": "node --debug-brk dist/server/index.js",
    "test": "ng test --configuration=local"
  },
  "pre-commit": [],
  "license": "MIT",
  "contributors": [],
  "dependencies": {
    "@angular/animations": "^6.1.6",
    "@angular/cdk": "^6.4.7",
    "@angular/common": "^6.1.6",
    "@angular/compiler": "^6.1.6",
    "@angular/core": "^6.1.6",
    "@angular/forms": "^6.1.6",
    "@angular/http": "^6.1.6",
    "@angular/material": "^6.4.3",
    "@angular/platform-browser": "^6.1.6",
    "@angular/platform-browser-dynamic": "^6.1.6",
    "@angular/platform-server": "^6.1.6",
    "@angular/pwa": "^0.7.5",
    "@angular/router": "^6.1.6",
    "@angular/service-worker": "^6.1.6",
    "@nguniversal/common": "^6.1.0",
    "@nguniversal/express-engine": "^6.1.0",
    "@nguniversal/module-map-ngfactory-loader": "^6.1.0",
    "@types/cookie-parser": "^1.4.1",
    "@types/decamelize": "^1.2.0",
    "bootstrap": "^4.1.3",
    "bowser": "^1.9.3",
    "class-transformer": "^0.1.9",
    "cookie-parser": "^1.4.3",
    "core-js": "^2.5.7",
    "decamelize": "^2.0.0",
    "font-awesome": "^4.7.0",
    "hammerjs": "^2.0.8",
    "linearicons": "^1.0.1",
    "node-sass": "^4.9.3",
    "rxjs": "^6.3.0",
    "ts-node": "^7.0.1",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.7.5",
    "@angular/cli": "^6.1.5",
    "@angular/compiler-cli": "^6.1.6",
    "@angular/language-service": "^6.1.6",
    "@types/express": "^4.16.0",
    "@types/google-maps": "^3.2.0",
    "@types/jasmine": "^2.8.8",
    "@types/node": "^10.9.4",
    "codelyzer": "^4.4.4",
    "cpy-cli": "^2.0.0",
    "express": "^4.16.3",
    "http-server": "^0.11.1",
    "istanbul-instrumenter-loader": "^3.0.1",
    "jasmine-core": "^3.2.1",
    "json-loader": "^0.5.7",
    "karma": "^3.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.2",
    "karma-coverage-istanbul-reporter": "^2.0.2",
    "karma-jasmine": "^1.1.2",
    "karma-jasmine-html-reporter": "^1.3.1",
    "karma-remap-istanbul": "^0.6.0",
    "karma-sourcemap-loader": "^0.3.7",
    "nodemon": "^1.18.4",
    "pre-commit": "^1.2.2",
    "reflect-metadata": "^0.1.12",
